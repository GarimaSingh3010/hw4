<% if @current_user%>

    <h1>Places New</h1>

    <ul>
      <% for place in @places %>
        <li><a href="/places/<%= place["id"] %>"><%= place["name"] %></a></li>
      <% end %>
    </ul>

    <p>
      <a href="/places/new">New place</a>
    </p>
<% else %>
    <p> You have to be logged in</p>
<%end%>